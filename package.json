{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "whop-proxy --command 'next dev --turbopack'",
    "build": "prisma generate && prisma migrate deploy && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "dev:mobile": "pnpm --dir apps/mobile start",
    "dev:all": "turbo run dev --parallel",
    "build:all": "turbo run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.16.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@vercel/functions": "^3.1.1",
    "@whop/api": "^0.0.51",
    "@whop/react": "^0.2.46",
    "next": "15.5.4",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@whop-apps/dev-proxy": "^0.0.1-canary.117",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "turbo": "^2.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.15.9"
}
